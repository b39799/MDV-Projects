$(document).ready(function(){
	$.ajax({
		"url": "_view/accounts",
		"dataType": "json",
		"success": function(data){
			$.each(data.rows, function(index, account){
				var account = account.value.accountName;
				var password = account.value.password;
				var username = account.value.user;
				var email = account.value.email;
				var notes = account.value.notes;
				$('#accountlist').append(
					$('<li>').text(account)
				
				);
			});
		}
	});
});